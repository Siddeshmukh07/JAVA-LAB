A}class Factorial {
    public static void main(String[] args) {
        int n = 5, fact = 1;

        for (int i = 1; i <= n; i++) {
            fact = fact * i;
        }

        System.out.println("Factorial = " + fact);
    }
}
B} class Prime {
    public static void main(String[] args) {
        int n = Integer.parseInt(args[0]);
        int flag = 0;

        for (int i = 2; i < n; i++) {
            if (n % i == 0) {
                flag = 1;
                break;
            }
        }

        if (flag == 0)
            System.out.println("Prime Number");
        else
            System.out.println("Not Prime");
    }
}
C}class Sort {
    public static void main(String[] args) {
        int a[] = {9, 5, 1, 4, 3, 8, 2, 7, 6, 0};

        for (int i = 0; i < 10; i++) {
            for (int j = i + 1; j < 10; j++) {
                if (a[i] > a[j]) {
                    int t = a[i];
                    a[i] = a[j];
                    a[j] = t;
                }
            }
        }

        for (int i = 0; i < 10; i++)
            System.out.print(a[i] + " ");
    }
}
D}class Merge {
    public static void main(String[] args) {
        int a[] = {1, 3, 5};
        int b[] = {2, 4, 6};
        int c[] = new int[6];

        int i = 0, j = 0, k = 0;

        while (i < 3 && j < 3) {
            if (a[i] < b[j])
                c[k++] = a[i++];
            else
                c[k++] = b[j++];
        }

        while (i < 3)
            c[k++] = a[i++];

        while (j < 3)
            c[k++] = b[j++];

        for (i = 0; i < 6; i++)
            System.out.print(c[i] + " ");
    }
}
E}class Matrix {
    public static void main(String[] args) {
        int A[][] = {{1,2},{3,4}};
        int B[][] = {{5,6},{7,8}};
        int C[][] = new int[2][2];
        int ch = 1;

        switch (ch) {
            case 1: // Addition
                for (int i = 0; i < 2; i++)
                    for (int j = 0; j < 2; j++)
                        C[i][j] = A[i][j] + B[i][j];
                break;

            case 2: // Multiplication
                for (int i = 0; i < 2; i++)
                    for (int j = 0; j < 2; j++)
                        C[i][j] = A[i][0]*B[0][j] + A[i][1]*B[1][j];
                break;

            case 3: // Transpose
                for (int i = 0; i < 2; i++)
                    for (int j = 0; j < 2; j++)
                        C[i][j] = A[j][i];
                break;
        }

        for (int i = 0; i < 2; i++) {
            for (int j = 0; j < 2; j++)
                System.out.print(C[i][j] + " ");
            System.out.println();
        }
    }
}
